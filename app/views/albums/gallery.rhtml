	<%= javascript_include_tag "thickbox" %>
	<%= stylesheet_link_tag "thickbox", :media => "screen" %>
	<%= image_tag "loadingAnimation.gif", {:style => 'display:none;'}%>
<% for photo in @photos %>
	<%= link_to(image_tag(url_for_file_column(photo,'image'), :size => '128x128'), url_for_file_column(photo,'image'), {:class => 'thickbox', :rel => photo.album.name, :title => photo.name }) %>
<% end %>