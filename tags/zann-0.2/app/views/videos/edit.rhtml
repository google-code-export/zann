<%= error_messages_for 'video' %>

<% form_tag({:action => 'update', :id => @video}, {:multipart => true}) do %>
<fieldset>
	<legend>Editing video</legend>
  <%= render :partial => 'form' %>
</fieldset>
<fieldset>
  <%= submit_tag 'Save', :class => "inputSubmit" %>
</fieldset>  
<% end %>

<%= link_to 'Show', :action => 'show', :id => @video %> |
<%= link_to 'Back', :action => 'list' %>
